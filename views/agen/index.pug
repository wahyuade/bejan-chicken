html
	head
		title Agen Dashboard
		script(src='/js/vue.js')
		script(src='/js/vue-router.js')
		script(src='/js/axios.min.js')
		link(rel="stylesheet", href="/css/bootstrap.css")
		link(rel="stylesheet", href="/css/simple-sidebar.css")
		link(rel="stylesheet", href="/css/bootstrap.min.css")
		link(rel="stylesheet", href="/css/bootstrap-datetimepicker.min.css")
		link(rel="stylesheet", href="/css/bootstrap-datetimepicker.min.css")
		link(rel="stylesheet", href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css")

	body
		div#agenApp
			div(id='wrapper')
				div(id='sidebar-wrapper')
					ul(class='sidebar-nav')
						li(class='sidebar-brand') Bejan's Poultry
						li(class='text-center' style='margin-left:-30px')
							img(class='img-circle' src='/img/farm.png' width='100px')
						li 
							router-link(to='/agen/pesanan') Pesanan
						li
							router-link(to='/agen/profile') Profile
						li
							a(href='#' v-on:click='doSignOut()') Sign Out
				div(id='page-content-wrapper')
					div(class='container-fluid')
						h1.text-center Welcome
						a(href='#menu-toggle' class='btn btn-secondary' id='menu-toggle') Menu
				router-view


		include pesanan.pug
		include profile.pug

		script(src='/js/agen.js')
		script(src='/js/jquery.min.js')
		script(src='/js/popper.min.js')
		script(src='/js/bootstrap.min.js')
		script(src='/js/toggle.js')
		script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js')
		script(src='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js')
		

