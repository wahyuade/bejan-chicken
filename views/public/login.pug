template#login-template
  div.container
    p Bejan's Chicken Farm Login
    form.form-signin(v-on:submit.prevent= 'doLogin()')
      input(
        type= 'text'
        v-model= 'no_telp'
        placeholder= 'email'
      )
      input(
        type= 'password'
        v-model= 'password'
        placeholder= 'password'
      )
      input(
        type= 'submit'
        value= 'Login'
      )